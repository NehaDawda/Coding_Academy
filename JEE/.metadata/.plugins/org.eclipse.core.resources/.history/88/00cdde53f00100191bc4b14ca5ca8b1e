package com.jp.hr.soap;

import javax.xml.ws.Endpoint;

import com.jp.hr.exceptions.HrException;
import com.jp.hr.viewInterfaces.EmpSoapServicesView;

//http://localhost:9898/hr?wsdl
public class PublishEmpServices {

	public static void main(String[] args) {
		try {
			EmpSoapServicesView soapServices = new EmpSoapServicesViewImpl();
			Endpoint.publish("http://localhost:9898/hr", soapServices);
			System.out.println("Services started");
		} catch (HrException e) {
			e.printStackTrace();
		}
		
	}

}
